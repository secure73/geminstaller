{
    "name": "gemvc/installer",
    "description": "Super light project based on Gemvc Framework",
    "type": "project",
    "require": {
        "gemvc/framework": "^3.0"
    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "App\\": "../../../app/",
            "APP\\Service\\": "../../../app/service/",
            "App\\Controller\\": "../../../app/controller/",
            "App\\Model\\": "../../../app/model/",
            "App\\Table\\": "../../../app/table/"
        }
    },
    "authors": [
        {
            "name": "Ali Khorsandfard",
            "email": "ali.khorsandfard@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "phpstan/phpstan": "^1.11",
        "pestphp/pest": "^2.34"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
